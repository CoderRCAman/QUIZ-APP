




<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="../css/style.css">
    <script src="https://unpkg.com/axios/dist/axios.min.js"></script>
</head> 

<body>
  
  <!-- As a heading
   --Process to edit/insert data to href link
   <a href="https://www.google.com/" onclick="location.href=this.href+'?xyz='+val;return false;">

  -->
  <nav class="navbar ">
       
    <a href="/profile/" onclick="location.href=this.href+'?name=<%=profile.name%>&email=<%=profile.email%>';return false;"> <%= profile.name %>  </a> 
    <span style="margin-left: 75%;">
    <a href="/home/" onclick="location.href=this.href+'?name=<%=profile.name%>&email=<%=profile.email%>';return false;"> <button class='btnn'>Home</button> </a>
    <a href="/logout"><button class="btnn">LogOut</button> </a>
    </span>
  </nav>
   <div class="main_div"> 
       <div class="inner_div">  
        <% if(profile.quizTook.size!==0) { %> 
            <h1>Available Quiz:</h1>
        <%}%>
       <% if(profile.quizTook.get('qit')===undefined) { %>
       <div style="margin-bottom: 10px;"><a href="/qit/" onclick="location.href=this.href+'?name=<%=profile.name%>&email=<%=profile.email%>';return false;"> Quiz on Information Technology  </a> </div>
       <% } %>
       <% if(profile.quizTook.get('qih')===undefined) { %>
       <div style="margin-bottom: 10px;"> <div style="margin-bottom: 10px;"><a href="/qih/" onclick="location.href=this.href+'?name=<%=profile.name%>&email=<%=profile.email%>';return false;"> Quiz on Physics </a> </div> </div>
       <% } %>
       <% if(profile.quizTook.get('qgw')===undefined) { %>
       <div style="margin-bottom: 10px;"> <div style="margin-bottom: 10px;"><a href="/qgw/" onclick="location.href=this.href+'?name=<%=profile.name%>&email=<%=profile.email%>';return false;"> Quiz on Chemistry  </a> </div></div>   
       <% } %> 
       <% if(profile.quizTook.size===3) { %>
       <div style="margin-bottom: 10px;"><a href="#">No Quiz Available </a></div> 
       <%}%>
    </div>
   </div>
    
   <script
   src="https://cdnjs.cloudflare.com/ajax/libs/qs/6.9.2/qs.min.js"
   integrity="sha256-TDxXjkAUay70ae/QJBEpGKkpVslXaHHayklIVglFRT4="
   crossorigin="anonymous"
 ></script>
   <script src="../js/script.js"></script> 
</body>
</html>